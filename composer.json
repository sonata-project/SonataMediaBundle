{
    "name": "sonata-project/media-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataMediaBundle",
    "keywords": ["media", "sonata", "youtube", "dailymotion", "vimeo", "pdf", "file", "upload"],
    "homepage": "http://sonata-project.org/bundles/media",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataMediaBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "2.1.x",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/notification-bundle": "dev-master",
        "sonata-project/easy-extends-bundle": "dev-master",
        "knplabs/gaufrette": "dev-master",
        "imagine/Imagine": "v0.3.0",
        "kriswallsmith/buzz": "0.7"
    },
    "require-dev": {
        "sonata-project/formatter-bundle": "dev-master"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "dev-master",
        "liip/imagine-bundle": "dev-master",
        "sonata-project/admin-bundle": "dev-master",
        "amazonwebservices/aws-sdk-for-php": "1.5.*"
    },
    "autoload": {
        "psr-0": { "Sonata\\MediaBundle": "" }
    },
    "target-dir": "Sonata/MediaBundle",
    "minimum-stability": "dev"
}
